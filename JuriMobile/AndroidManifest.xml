<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.unisinos.jurimobile"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.unisinos.jurimobile.controller.ProcessoListActivity"
            android:label="@string/meusProcessos" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.unisinos.jurimobile.controller.ProcessoActivity"
            android:label="@string/processo" >
        </activity>
        <activity
            android:name="br.unisinos.jurimobile.controller.MovimentoActivity"
            android:label="@string/movimento" >
        </activity>
    </application>

</manifest>